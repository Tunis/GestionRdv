<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="400.0"
            xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="app.controller.dialog.ProfilPatientDialogCtrl">
   <center>
       <TableView fx:id="tableRdv" minHeight="200.0" minWidth="200.0" prefHeight="200.0" BorderPane.alignment="CENTER">
        <columns>
            <TableColumn fx:id="colDate" editable="false" prefWidth="75.0" sortType="DESCENDING" text="Date"/>
            <TableColumn fx:id="colCat" editable="false" prefWidth="75.0" text="Catégorie"/>
            <TableColumn fx:id="ColMed" editable="false" prefWidth="75.0" text="Medecin"/>
            <TableColumn fx:id="colPay" editable="false" prefWidth="75.0" sortType="DESCENDING" text="Payé"/>
        </columns>
         <BorderPane.margin>
             <Insets bottom="10.0" left="5.0" right="5.0" top="10.0"/>
         </BorderPane.margin>
           <columnResizePolicy>
               <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
           </columnResizePolicy>
      </TableView>
   </center>
   <top>
       <VBox alignment="CENTER" minWidth="400.0" BorderPane.alignment="CENTER">
           <BorderPane.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
           </BorderPane.margin>
           <children>
               <GridPane alignment="CENTER" hgap="5.0" maxHeight="1.7976931348623157E308"
