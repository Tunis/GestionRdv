                                  GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER"/>
                       <TextField fx:id="textFNomJF" editable="false" minHeight="30.0" GridPane.columnIndex="1"
                                  GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2"
                                  GridPane.valignment="CENTER"/>
                       <DatePicker fx:id="dpDate" editable="false" minHeight="30.0" GridPane.columnIndex="1"
                                   GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3"
                                   GridPane.valignment="CENTER"/>
                       <TextField fx:id="textFAdresse" minHeight="30.0" GridPane.columnIndex="1"
                                  GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4"
                                  GridPane.valignment="CENTER"/>
                       <TextField fx:id="textFVille" minHeight="30.0" GridPane.columnIndex="1"
                                  GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="5"
                                  GridPane.valignment="CENTER"/>
                       <TextField fx:id="textFCP" minHeight="30.0" GridPane.columnIndex="1" GridPane.halignment="LEFT"
                                  GridPane.hgrow="ALWAYS" GridPane.rowIndex="6" GridPane.valignment="CENTER"/>
