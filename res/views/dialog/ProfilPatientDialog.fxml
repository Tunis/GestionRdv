                           </padding>
                       </Label>
                       <Label minHeight="30.0" text="Ville" GridPane.halignment="RIGHT" GridPane.hgrow="ALWAYS"
                              GridPane.rowIndex="5" GridPane.valignment="CENTER">
                           <padding>
                               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                           </padding>
                       </Label>
                       <Label minHeight="30.0" text="CP" GridPane.halignment="RIGHT" GridPane.hgrow="ALWAYS"
                              GridPane.rowIndex="6" GridPane.valignment="CENTER">
                           <padding>
                               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                           </padding>
                       </Label>
                       <Label minHeight="30.0" text="Mail" GridPane.halignment="RIGHT" GridPane.hgrow="ALWAYS"
                              GridPane.rowIndex="7" GridPane.valignment="CENTER">
                           <padding>
                               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
