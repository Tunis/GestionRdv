   </top>
   <bottom>
      <Button fx:id="btnSubmit" mnemonicParsing="false" onAction="#handleSubmit" text="Valider" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
</BorderPane>
