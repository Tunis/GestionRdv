                                  GridPane.hgrow="ALWAYS" GridPane.rowIndex="8" GridPane.valignment="CENTER"/>
                       <TextField fx:id="textFNSecu" minHeight="30.0" GridPane.columnIndex="1"
                                  GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="9"
                                  GridPane.valignment="CENTER"/>
