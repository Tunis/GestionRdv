<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<BorderPane prefHeight="450.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.TabPaiementOverviewCtrl">
   <center>
       <HBox alignment="CENTER" maxHeight="450.0" maxWidth="1.7976931348623157E308" spacing="10.0"
             BorderPane.alignment="CENTER">
         <children>
             <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0"
                   HBox.hgrow="NEVER">
               <children>
                   <Label fx:id="lblDate" alignment="CENTER" contentDisplay="CENTER" text="Jour X"/>
                   <ListView fx:id="listRdvDay" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                             VBox.vgrow="ALWAYS"/>
               </children>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </VBox>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <children>
                   <TextField fx:id="searchField">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </TextField>
                   <ListView fx:id="listPaiement" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                   </ListView>
               </children>
            </VBox>
         </children>
           <BorderPane.margin>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
           </BorderPane.margin>
      </HBox>
   </center>
</BorderPane>
