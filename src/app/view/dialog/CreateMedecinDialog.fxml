<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="app.controller.dialog.CreateMedecinDialogCtrl">
   <center>
       <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
             <GridPane hgap="5.0">
              <columnConstraints>
                  <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" minWidth="10.0"/>
                  <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" minWidth="10.0"/>
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
              </rowConstraints>
               <children>
                   <Label text="Nom">
                       <GridPane.margin>
                           <Insets bottom="5.0" top="5.0"/>
                       </GridPane.margin>
                   </Label>
                   <Label text="Prenom" GridPane.rowIndex="1">
                       <GridPane.margin>
                           <Insets bottom="5.0" top="5.0"/>
                       </GridPane.margin>
                   </Label>
                   <Label text="Tel." GridPane.rowIndex="2">
                       <GridPane.margin>
                           <Insets bottom="5.0" top="5.0"/>
                       </GridPane.margin>
                   </Label>
                   <Label text="Mail" GridPane.rowIndex="3">
                       <GridPane.margin>
                           <Insets bottom="5.0" top="5.0"/>
                       </GridPane.margin>
                   </Label>
                   <TextField fx:id="textFNom" minWidth="200.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS">
                       <GridPane.margin>
                           <Insets bottom="5.0" top="5.0"/>
                       </GridPane.margin>
                   </TextField>
                   <TextField fx:id="textFPrenom" minWidth="200.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS"
                              GridPane.rowIndex="1">
                       <GridPane.margin>
                           <Insets bottom="5.0" top="5.0"/>
                       </GridPane.margin>
                   </TextField>
                   <TextField fx:id="textFTel" minWidth="200.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS"
                              GridPane.rowIndex="2">
                       <GridPane.margin>
                           <Insets bottom="5.0" top="5.0"/>
                       </GridPane.margin>
                   </TextField>
                   <TextField fx:id="textFMail" minWidth="200.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS"
                              GridPane.rowIndex="3">
                       <GridPane.margin>
                           <Insets bottom="5.0" top="5.0"/>
                       </GridPane.margin>
                   </TextField>
               </children>
                 <VBox.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                 </VBox.margin>
            </GridPane>
             <Button fx:id="btnSubmit" mnemonicParsing="false" onAction="#handleSubmit" text="Valider"/>
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
