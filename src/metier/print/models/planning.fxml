<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane fx:controller="metier.print.models.PlanningCtrl" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <VBox alignment="TOP_CENTER" spacing="5.0" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="TOP_CENTER" fillHeight="false" spacing="10.0">
                    <children>
                        <Label text="Medecin :"/>
                        <Label fx:id="lblMedecin" text="Label"/>
                    </children>
                </HBox>
                <HBox alignment="TOP_CENTER" fillHeight="false" spacing="10.0">
                    <children>
                        <Label text="Date :"/>
                        <Label fx:id="lblDate" text="Label"/>
                    </children>
                </HBox>
            </children>
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </BorderPane.margin>
        </VBox>
    </top>
    <center>
        <TableView fx:id="tblJour" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn fx:id="colTime" editable="false" prefWidth="75.0" sortable="false" text="Heure"/>
                <TableColumn fx:id="colPatient" editable="false" prefWidth="75.0" sortable="false" text="Patient">
                    <columns>
                        <TableColumn fx:id="colPatientNom" editable="false" prefWidth="75.0" sortable="false"
                                     text="Nom"/>
                        <TableColumn fx:id="colPatientNomJF" editable="false" prefWidth="75.0" sortable="false"
                                     text="Nom de JF"/>
                        <TableColumn fx:id="colPatientPrenom" editable="false" prefWidth="75.0" sortable="false"
                                     text="Prenom"/>
                    </columns>
                </TableColumn>
                <TableColumn fx:id="ColCS" editable="false" prefWidth="75.0" sortable="false" text="Type Rdv"/>
                <TableColumn fx:id="colCotation" editable="false" prefWidth="75.0" sortable="false" text="Cotation"/>
                <TableColumn fx:id="ColTP" editable="false" prefWidth="75.0" sortable="false" text="TP"/>
                <TableColumn fx:id="colEspece" editable="false" prefWidth="75.0" sortable="false" text="Espece"/>
                <TableColumn fx:id="colCB" editable="false" prefWidth="75.0" sortable="false" text="CB"/>
                <TableColumn fx:id="colCheque" editable="false" prefWidth="75.0" sortable="false" text="Cheque">
                    <columns>
                        <TableColumn fx:id="colMontantCheque" editable="false" prefWidth="75.0" sortable="false"
                                     text="Montant"/>
                        <TableColumn fx:id="colBanqueCheque" editable="false" prefWidth="75.0" sortable="false"
                                     text="Banque"/>
                        <TableColumn fx:id="colNomCheque" editable="false" prefWidth="75.0" sortable="false"
                                     text="Nom"/>
                    </columns>
                </TableColumn>
                <TableColumn fx:id="colImpayer" editable="false" prefWidth="75.0" sortable="false" text="C1"/>
            </columns>
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </BorderPane.margin>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </center>
    <bottom>
        <GridPane alignment="TOP_CENTER" hgap="50.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints fillWidth="false" halignment="CENTER" hgrow="ALWAYS" minWidth="10.0"/>
                <ColumnConstraints fillWidth="false" halignment="RIGHT" hgrow="ALWAYS" minWidth="10.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="100.0" minHeight="10.0" prefHeight="75.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <GridPane alignment="CENTER" gridLinesVisible="true" GridPane.halignment="CENTER"
                          GridPane.valignment="CENTER">
                    <columnConstraints>
                        <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" minWidth="50.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="50.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints fillHeight="false" vgrow="NEVER"/>
                        <RowConstraints fillHeight="false" vgrow="NEVER"/>
                        <RowConstraints fillHeight="false" vgrow="NEVER"/>
                        <RowConstraints fillHeight="false" vgrow="NEVER"/>
                    </rowConstraints>
                    <children>
                        <Label text="CS"/>
                        <Label text="C2" GridPane.rowIndex="1"/>
                        <Label text="ECHO" GridPane.rowIndex="2"/>
                        <Label text="DIU" GridPane.rowIndex="3"/>
                        <Label fx:id="nbCS" GridPane.columnIndex="1"/>
                        <Label fx:id="nbC2" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                        <Label fx:id="nbECHO" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                        <Label fx:id="nbDIU" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                    </children>
                </GridPane>
                <TableView fx:id="tblTotal" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.hgrow="NEVER"
                           GridPane.valignment="CENTER" GridPane.vgrow="NEVER">
                    <columns>
                        <TableColumn fx:id="colDate" editable="false" prefWidth="75.0" sortable="false" text="C1"/>
                        <TableColumn fx:id="ColTPTotal" editable="false" prefWidth="75.0" sortable="false" text="TP">
                            <columns>
                                <TableColumn fx:id="colTnbTP" editable="false" prefWidth="75.0" sortable="false"
                                             text="Nb"/>
                                <TableColumn fx:id="colTMontantTP" editable="false" prefWidth="75.0" sortable="false"
                                             text="Montant"/>
                            </columns>
                        </TableColumn>
                        <TableColumn fx:id="colEspeceTotal" editable="false" prefWidth="75.0" sortable="false"
                                     text="Especes">
                            <columns>
                                <TableColumn fx:id="colTNbEspece" editable="false" prefWidth="75.0" sortable="false"
                                             text="Nb"/>
                                <TableColumn fx:id="colTMontantEspece" editable="false" prefWidth="75.0"
                                             sortable="false" text="Montant"/>
                            </columns>
                        </TableColumn>
                        <TableColumn fx:id="colCBTotal" editable="false" prefWidth="75.0" sortable="false" text="CB">
                            <columns>
                                <TableColumn fx:id="colTNbCB" editable="false" prefWidth="75.0" sortable="false"
                                             text="Nb"/>
                                <TableColumn fx:id="colTMontantCB" editable="false" prefWidth="75.0" sortable="false"
                                             text="Montant"/>
                            </columns>
                        </TableColumn>
                        <TableColumn fx:id="colChequeTotal" editable="false" prefWidth="75.0" sortable="false"
                                     text="Cheques">
                            <columns>
                                <TableColumn fx:id="colTNbCheque" editable="false" prefWidth="75.0" sortable="false"
                                             text="Nb"/>
                                <TableColumn fx:id="colTMontantCheque" editable="false" prefWidth="75.0"
                                             sortable="false" text="Montant"/>
                            </columns>
                        </TableColumn>
                        <TableColumn fx:id="colImpayerTotal" editable="false" prefWidth="75.0" sortable="false"
                                     text="Impayer"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </TableView>
            </children>
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </BorderPane.margin>
        </GridPane>
    </bottom>
</BorderPane>
